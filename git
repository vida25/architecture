[32m+[m[32mimport { DomSanitizer, SafeResourceUrl, SafeUrl} from '@angular/platform-browser';[m
[32m+[m[32mimport { Input, Component, OnInit, OnDestroy } from '@angular/core';[m
[32m+[m[32mimport { ActivatedRoute, Router }  from '@angular/router';[m
[32m+[m[32mimport { Location } from '@angular/common';[m
[32m+[m
[32m+[m[32mimport { ApatrmentAboutService } from '../../services/apartment-about/apartment-about.service';[m
[32m+[m[32mimport { apartmentInterface } from '../../interfaces/apartment.interface';[m
[32m+[m
[32m+[m[32mimport * as _ from 'underscore';[m
[32m+[m
[32m+[m[32m@Component({[m
[32m+[m[32m  moduleId: module.id,[m
[32m+[m[32m  selector: "floor-component",[m
[32m+[m[32m  templateUrl: "./floor.component.html",[m
[32m+[m[32m  styleUrls: [ "./floor.component.css" ],[m
[32m+[m[32m})[m
[32m+[m[32mexport class FloorComponent implements OnInit {[m
[32m+[m[32m  private sub: any;[m
[32m+[m
[32m+[m[32m  public apartmentsInformation: any = null;[m
[32m+[m[32m  public aboutApartment: any;[m
[32m+[m[32m  public information: string;[m
[32m+[m
[32m+[m[32m  public sectionId: string;[m
[32m+[m[32m  public floorId: string;[m
[32m+[m[32m  public houseId: string;[m
[32m+[m
[32m+[m[32m  constructor([m
[32m+[m[32m    private apatrmentAbout: ApatrmentAboutService,[m
[32m+[m[32m    private domSanitizer: DomSanitizer,[m
[32m+[m[32m    private route: ActivatedRoute,[m
[32m+[m[32m    private location: Location,[m
[32m+[m[32m    private router: Router[m
[32m+[m[32m  ) {}[m
[32m+[m
[32m+[m[32m  ngOnInit() {[m
[32m+[m[32m    this.sub = this.route.params.subscribe((params: any) => {[m
[32m+[m[32m          this.sectionId = params['id1'];[m
[32m+[m[32m          this.houseId = params['id2'];[m
[32m+[m[32m          this.floorId = params['id3'];[m
[32m+[m[32m    });[m
[32m+[m[32m    this.getAboutApartmentData('apartments').then( res => {[m
[32m+[m[32m        this.apartmentsInformation = res;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  showData(apartment) {[m
[32m+[m[32m      this.aboutApartment = {[m
[32m+[m[32m        square: apartment.aboutApartment[0].value,[m
[32m+[m[32m        room: apartment.roomCount,[m
[32m+[m[32m      }[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  replaceSpace(str: string) {[m
[32m+[m[32m    return str.replace(/ /ig, ",");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getAboutApartmentData(id:any = null) {[m
[32m+[m[32m      return this.apatrmentAbout.getApartments( id );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  hideData() {[m
[32m+[m[32m    this.aboutApartment = null;[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  ngOnDestroy() {[m
[32m+[m[32m    this.sub.unsubscribe();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  goToApartment(id) {[m
[32m+[m[32m    let order = id.match(/\d+$/);[m
[32m+[m[32m    let url = `/house/${this.houseId}/section/${this.sectionId}/floor/${this.floorId}/apartment/${order}`;[m
[32m+[m[32m    this.router.navigate([url]);[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m}[m
