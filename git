[36m@@ -0,0 +1,22 @@[m
[32m+[m[32mimport { Directive, ElementRef, Input, AfterViewInit } from '@angular/core';[m
[32m+[m
[32m+[m[32m@Directive({[m
[32m+[m[32m  selector: '[polygonDirective]',[m
[32m+[m[32m  host: { }[m
[32m+[m[32m})[m
[32m+[m[32mexport class PolygonDirective implements AfterViewInit {[m
[32m+[m[32m  private el:HTMLElement;[m
[32m+[m[32m  private browserDomAdapter: any;[m
[32m+[m
[32m+[m[32m  @Input('polygonDirective') points: string;[m
[32m+[m
[32m+[m[32m  constructor( el: ElementRef ) {[m
[32m+[m[32m    this.el = el.nativeElement;[m
[32m+[m[32m  }[m
[32m+[m[32m  ngAfterViewInit() {[m
[32m+[m[32m    this.polygonMaker();[m
[32m+[m[32m  }[m
[32m+[m[32m  private polygonMaker() {[m
[32m+[m[32m    this.el.innerHTML = `<polygon points="${this.points}"></polygon>`;[m
[32m+[m[32m  }[m
[32m+[m[32m}[m
