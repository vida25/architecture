[32m+[m[32mimport { Http, Response } from '@angular/http';[m
[32m+[m[32mimport { Injectable } from '@angular/core';[m
[32m+[m
[32m+[m[32mimport { apartmentInterface } from '../../interfaces/apartment.interface';[m
[32m+[m
[32m+[m[32mimport 'rxjs/add/operator/toPromise';[m
[32m+[m[32mimport * as _ from 'lodash';[m
[32m+[m
[32m+[m
[32m+[m[32m@Injectable()[m
[32m+[m[32mexport class ApatrmentAboutService {[m
[32m+[m[32m    private defValue: string = 'apartments';[m
[32m+[m
[32m+[m[32m    constructor (private http: Http) {}[m
[32m+[m
[32m+[m[32m    urlMaker(id: any) {[m
[32m+[m[32m      let idPassed = _.isEqual(id, null)[m
[32m+[m[32m            ? this.defValue[m
[32m+[m[32m            : id;[m
[32m+[m[32m      return `./src/app/data/about-apartment/${ idPassed }.json`;[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    getApartments(idPassed: any): Promise<any> {[m
[32m+[m[32m      return this.http.get(this.urlMaker(idPassed))[m
[32m+[m[32m             .toPromise()[m
[32m+[m[32m             .then(response => response.json())[m
[32m+[m[32m             .catch(this.handleError);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m    private handleError (error: Response | any) {[m
[32m+[m[32m      return Promise.reject(error.message || error);[m
[32m+[m[32m    };[m
[32m+[m
[32m+[m[32m}[m
