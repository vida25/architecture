[32m+[m[32mimport { Component, OnInit, OnDestroy } from '@angular/core';[m
[32m+[m[32mimport { ActivatedRoute, Router } from '@angular/router';[m
[32m+[m[32mimport { Location } from '@angular/common';[m
[32m+[m
[32m+[m[32mimport { ApatrmentAboutService } from '../../services/apartment-about/apartment-about.service';[m
[32m+[m[32mimport { apartmentInterface } from '../../interfaces/apartment.interface';[m
[32m+[m
[32m+[m[32mimport * as _ from 'underscore';[m
[32m+[m
[32m+[m[32m@Component({[m
[32m+[m[32m  moduleId: module.id,[m
[32m+[m[32m  selector: "apartment-component",[m
[32m+[m[32m  templateUrl: "./apartment.component.html",[m
[32m+[m[32m  styleUrls: [ "./apartment.component.css" ][m
[32m+[m[32m})[m
[32m+[m[32mexport class ApartmentComponent {[m
[32m+[m[32m  private sub: any;[m
[32m+[m
[32m+[m[32m  public dimentions: dimentionInterface[] = Dimentions;[m
[32m+[m[32m  public active: dimentionInterface = Dimentions[0];[m
[32m+[m[32m  public apartmentsStorage: any = null;[m
[32m+[m[32m  public currentApartment: any = null;[m
[32m+[m[32m  public apartmentSrc: string;[m
[32m+[m[32m  public apartmentId: string;[m
[32m+[m
[32m+[m[32m  constructor([m
[32m+[m[32m    private apatrmentAbout: ApatrmentAboutService,[m
[32m+[m[32m    private route: ActivatedRoute,[m
[32m+[m[32m    private location: Location,[m
[32m+[m[32m    private router: Router,[m
[32m+[m[32m  ) {}[m
[32m+[m
[32m+[m[32m  ngOnInit() {[m
[32m+[m[32m    this.sub = this.route.params.subscribe(params => {[m
[32m+[m[32m      this.apartmentId = `apartment${params['id']}`;[m
[32m+[m[32m       // In a real app: dispatch action to load.[m
[32m+[m[32m    });[m
[32m+[m[32m    this.getAboutApartmentData().then( res => {[m
[32m+[m[32m      this.apartmentsStorage = res;[m
[32m+[m[32m      this.defCurrentApartment();[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getAboutApartmentData(id:any = null) {[m
[32m+[m[32m      return this.apatrmentAbout.getApartments( id );[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  defCurrentApartment() {[m
[32m+[m[32m    this.currentApartment = _.find([m
[32m+[m[32m          this.apartmentsStorage,[m
[32m+[m[32m          apartment =>_.isEqual(apartment.id, this.apartmentId)[m
[32m+[m[32m    );[m
[32m+[m[32m    this.apartmentSrc = this.currentApartment['2d'];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  changeView(d: dimentionInterface) {[m
[32m+[m[32m    this.active = d;[m
[32m+[m[32m    this.apartmentSrc = this.currentApartment[d.dim];[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  goBack() {[m
[32m+[m[32m    this.location.back();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  ngOnDestroy() { this.sub.unsubscribe(); }[m
[32m+[m
[32m+[m[32m}[m
[32m+[m
[32m+[m
[32m+[m[32mexport const Dimentions: dimentionInterface[] = [[m
[32m+[m[32m  {dim: '2d'},[m
[32m+[m[32m  {dim: '3d'}[m
[32m+[m[32m][m
[32m+[m
[32m+[m[32mexport interface dimentionInterface {[m
[32m+[m[32m  dim: string;[m
[32m+[m[32m}[m
