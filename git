[32m+[m[32mimport { Component, OnInit, OnDestroy } from '@angular/core';[m
[32m+[m[32mimport { ActivatedRoute, Router } from '@angular/router';[m
[32m+[m
[32m+[m[32mimport { SectionsAboutService } from '../../services/section/section.service';[m
[32m+[m[32mimport { SvgMenuComponent } from '../svg-menu/svg-menu.component';[m
[32m+[m
[32m+[m[32mimport * as _ from 'underscore';[m
[32m+[m
[32m+[m[32m@Component({[m
[32m+[m[32m  moduleId: module.id,[m
[32m+[m[32m  selector: 'house-component',[m
[32m+[m[32m  templateUrl: './house.component.html',[m
[32m+[m[32m  styleUrls: ['./house.component.css'][m
[32m+[m[32m})[m
[32m+[m[32mexport class HouseComponent implements OnInit, OnDestroy {[m
[32m+[m[32m  public selectedMenuItem: any = null;[m
[32m+[m[32m  private sections: any = null;[m
[32m+[m[32m  private houseId: number;[m
[32m+[m[32m  private sub: any;[m
[32m+[m
[32m+[m[32m  constructor([m
[32m+[m[32m    private sectionsService: SectionsAboutService,[m
[32m+[m[32m    private route: ActivatedRoute,[m
[32m+[m[32m    private router: Router[m
[32m+[m[32m  ) {}[m
[32m+[m
[32m+[m[32m  ngOnInit() {[m
[32m+[m[32m    this.sub = this.route.params.subscribe(params => {[m
[32m+[m[32m       this.houseId = +params['id'];[m
[32m+[m
[32m+[m[32m      // In a real app: dispatch action to load the details here.[m
[32m+[m[32m    });[m
[32m+[m
[32m+[m[32m    this.getSections();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  getSections() {[m
[32m+[m[32m    this.sectionsService.getSections().then( res => {[m
[32m+[m[32m      this.sections = res;[m
[32m+[m[32m    });[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  replaceSpace(str: string) {[m
[32m+[m[32m    return str.replace(/ /ig, ",");[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m  makeVisibleSvgMenu( section ): void {[m
[32m+[m[32m    this.selectedMenuItem = _.isEqual(section.sectionId, this.selectedMenuItem)[m
[32m+[m[32m            ? null[m
[32m+[m[32m            : section.sectionId;[m
[32m+[m[32m  }[m
[32m+[m[32m  isEqual(a, b) {[m
[32m+[m[32m    return _.isEqual(a,b);[m
[32m+[m[32m  }[m
[32m+[m[32m  ngOnDestroy() {[m
[32m+[m[32m    this.sub.unsubscribe();[m
[32m+[m[32m  }[m
[32m+[m
[32m+[m[32m}[m
